{
  "name": "darkanakin41/stream-bundle",
  "type": "symfony-bundle",
  "description": "Pierre LEJEUNE StreamBundle",
  "keywords": [
    "symfony",
    "stream",
    "twitch",
    "bundle"
  ],
  "homepage": "https://github.com/darkanakin41/stream-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Pierre LEJEUNE",
      "email": "darkanakin41@gmail.com"
    }
  ],
  "require": {
    "php": "^7",
    "darkanakin41/core-bundle": "dev-master",
    "darkanakin41/api-bundle": "dev-master",
    "symfony/framework-bundle": "*",
    "symfony/orm-pack": "*",
    "symfony/validator": "*",
    "symfony/yaml": "*"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Darkanakin41\\StreamBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Darkanakin41\\StreamBundle\\Tests\\": "tests/"
    }
  }
}
